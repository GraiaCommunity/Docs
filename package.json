{
  "name": "vitepress-starter",
  "description": "Graia 社区文档",
  "repository": "https://github.com/GraiaCommunity/Docs",
  "keywords": [
    "graia-ariadne",
    "graia-project",
    "graiax"
  ],
  "author": "GraiaCommunity",
  "license": "MIT",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "serve": "vitepress serve docs",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write docs/",
    "minfont": "node scripts/minfont.cjs"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/hash-sum": "^1.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/markdown-it-footnote": "^3.0.0",
    "@types/mermaid": "^9.2.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.41.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "fontmin": "^0.9.9",
    "hash-sum": "^2.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.3",
    "mermaid": "^9.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup-plugin-external-globals": "^0.7.3",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vitepress": "1.0.0-beta.1",
    "vue": "^3.3.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom",
        "rollup"
      ]
    }
  }
}
